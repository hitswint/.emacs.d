clear
daemonize

super+n start
Escape end
Ctrl+g end
q record ~/.keynav_macros       #q+x
shift+at playback               #@+x
a history-back

h cut-left
j cut-down
k cut-up
l cut-right
shift+h move-left
shift+j move-down
shift+k move-up
shift+l move-right
semicolon cut-left,cut-up
apostrophe cut-right,cut-up
comma cut-left,cut-down
period cut-right,cut-down
shift+semicolon move-left,move-up
shift+apostrophe move-right,move-up
shift+comma move-left,move-down
shift+period move-right,move-down

space warp,click 1,end
u warp,click 1,end
i warp,click 2,end
o warp,click 3,end
shift+u click 1
shift+i click 2
shift+o click 3
slash warp,end

w windowzoom
c cursorzoom 200 200
m cursorzoom 5 5

p warp,drag 1
ctrl+p warp,drag 1 ctrl
shift+p warp,drag 1 shift
ctrl+shift+p warp,drag 1 ctrl+shift
alt+p warp,drag 1 alt

super+shift+n start, grid 3x3
1 cell-select 1x1
2 cell-select 2x1
3 cell-select 3x1
4 cell-select 1x2
5 cell-select 2x2
6 cell-select 3x2
7 cell-select 1x3
8 cell-select 2x3
9 cell-select 3x3
