URxvt.perl-ext-common: default,tabbed,keyboard-select,selection-to-clipboard
! C-M-c/C-M-v Â§çÂà∂Á≤òË¥¥Âà∞clipboard„ÄÇ
URxvt.tabbed.tabbar-fg: 5
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 6
URxvt.tabbed.tab-bg: 0
URxvt.keysym.S-Escape: perl:keyboard-select:activate
