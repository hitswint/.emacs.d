!!=============================================================================
!! RXVT-unicode setting
!!=============================================================================
Xft.dpi:96
URxvt.background:black
URxvt.foreground:white
URxvt.colorBD:yellow
URxvt.colorUL:green
URxvt.multichar_encoding:utf-8
URxvt.saveLines:10000
URxvt.color12:DodgerBlue
URxvt.menu:/etc/X11/rxvt.menu
URxvt.preeditType:Root
URxvt.scrollBar:False
! URxvt.font: -*-fixed-medium-r-*--18-*-*-*-*-*-iso10646-1,xft:WenQuanYi Bitmap Song:pixelsize=14:antialias=false
URxvt.font: -*-fixed-medium-r-*--18-*-*-*-*-*-iso10646-1,xft:WenQuanYi Micro Hei Mono:pixelsize=14:antialias=true
URxvt.iso14755: False
URxvt*iso14755_52: False
URxvt.searchable-scrollback: CM-s
! URxvt.keysym.CM-s: searchable-scrollback:start

! URxvt.iconFile:/usr/share/icons/Adwaita/scalable/legacy/utilities-terminal-symbolic.svg
URxvt.iconFile:/usr/share/icons/Adwaita/48x48/legacy/utilities-terminal.png

! perl-ext-common.
! https://github.com/muennich/urxvt-perls
